@prefix nbib:     <http://www.example.com/nbib#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix nrl:     <http://www.semanticdesktop.org/ontologies/2007/08/15/nrl#> .
@prefix nie:     <http://www.semanticdesktop.org/ontologies/2007/01/19/nie#> .
@prefix nfo:     <http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#> .
@prefix nco:     <http://www.semanticdesktop.org/ontologies/2007/03/22/nco#> .
@prefix nao:     <http://www.semanticdesktop.org/ontologies/2007/08/15/nao#> .

nbib: {
    nbib:Reference
          a       rdfs:Class ;
          rdfs:comment "A bibliographic reference" ;
          rdfs:label "Bibliographic Reference" ;
          rdfs:subClassOf nie:InformationElement .

####################################################################
#
# Resource Types
# these can be translated back into BibTeX entries

    nbib:Publication
          a       rdfs:Class ;
          rdfs:comment "A bibliographic reference resource type (article, book, paper and such)" ;
          rdfs:label "Bibliographic Resource" ;
          rdfs:subClassOf nie:InformationElement .

    nbib:Article
          a       rdfs:Class ;
          rdfs:comment "An article from a journal or magazine." ;
          rdfs:label "Article" ;
          rdfs:subClassOf nbib:Publication .

    nbib:BachelorThesis
          a       rdfs:Class ;
          rdfs:comment "A Bachelor thesis." ;
          rdfs:label "Bachelorthesis" ;
          rdfs:subClassOf nbib:Thesis .

    nbib:Book
          a       rdfs:Class ;
          rdfs:comment "A book with an explicit publisher." ;
          rdfs:label "Book" ;
          rdfs:subClassOf nbib:Publication .

    nbib:Booklet
          a       rdfs:Class ;
          rdfs:comment "A work that is printed and bound, but without a named publisher or sponsoring institution." ;
          rdfs:label "Booklet" ;
          rdfs:subClassOf nbib:Publication .

    nbib:Collection
          a       rdfs:Class ;
          rdfs:comment "A collection of different publications." ;
          rdfs:label "Collection" ;
          rdfs:subClassOf nbib:Publication .

    nbib:Electronic
          a       rdfs:Class ;
          rdfs:comment "An electronic publication (for example website or presentations)" ;
          rdfs:label "Electronic source" ;
          rdfs:subClassOf nbib:Publication .

    nbib:Manual
          a       rdfs:Class ;
          rdfs:comment "Technical documentation." ;
          rdfs:label "Manual" ;
          rdfs:subClassOf nbib:Publication .

    nbib:MastersThesis
          a       rdfs:Class ;
          rdfs:comment "A Master's thesis." ;
          rdfs:label "Mastersthesis" ;
          rdfs:subClassOf nbib:Thesis .

    nbib:PhdThesis
          a       rdfs:Class ;
          rdfs:comment "A PhD thesis." ;
          rdfs:label "Phdthesis" ;
          rdfs:subClassOf nbib:Thesis .

    nbib:Presentation
          a       rdfs:Class ;
          rdfs:comment "Slides for a presentation" ;
          rdfs:label "Presentation" ;
          rdfs:subClassOf nbib:Publication .

    nbib:Proceedings
          a       rdfs:Class ;
          rdfs:comment "The proceedings of a conference." ;
          rdfs:label "Proceedings" ;
          rdfs:subClassOf nbib:Collection .

    nbib:Script
          a       rdfs:Class ;
          rdfs:comment "The script for something (a lecture for ecxample)" ;
          rdfs:label "Script" ;
          rdfs:subClassOf nbib:Publication .

    nbib:Techreport
          a       rdfs:Class ;
          rdfs:comment "A report published by a school or other institution, usually numbered within a series." ;
          rdfs:label "Techreport" ;
          rdfs:subClassOf nbib:Publication .

    nbib:Thesis
          a       rdfs:Class ;
          rdfs:comment "A general thesis." ;
          rdfs:label "Thesis" ;
          rdfs:subClassOf nbib:Publication .

    nbib:Unpublished
          a       rdfs:Class ;
          rdfs:comment "A document having an author and title, but not formally published." ;
          rdfs:label "Unpublished" ;
          rdfs:subClassOf nbib:Publication .

    nbib:Patent
          a       rdfs:Class ;
          rdfs:comment "A patent." ;
          rdfs:label "Patent" ;
          rdfs:subClassOf nbib:Publication .

####################################################################
#
# Helper classes

    nbib:Chapter
          a       rdfs:Class ;
          rdfs:comment "A chapter of a publication." ;
          rdfs:label "Chapter" ;
          rdfs:subClassOf nie:InformationElement .

    nbib:JournalIssue
          a       rdfs:Class ;
          rdfs:comment "A journal issue for serious, scholarly publications" ;
          rdfs:label "JournalIssue" ;
          rdfs:subClassOf nbib:Collection .

    nbib:Series
          a       rdfs:Class ;
          rdfs:comment "Superclass to represent a series of some published data. (Book Series, Journal with several issues etc)" ;
          rdfs:label "Series" ;
          rdfs:subClassOf nie:InformationElement .

    nbib:BookSeries
          a       rdfs:Class ;
          rdfs:comment "The series of a book." ;
          rdfs:label "BookSeries" ;
          rdfs:subClassOf nbib:Series .

    nbib:Journal
          a       rdfs:Class ;
          rdfs:comment "A Journal which publish single journal issues." ;
          rdfs:label "Journal" ;
          rdfs:subClassOf nbib:Series .

####################################################################
#
# Properties

   nbib:publishedAs
         a       rdf:Property ;
         rdfs:domain nfo:Document ;
         rdfs:comment "Links a document to its publication information." ;
         rdfs:label "publishedAs" ;
         rdfs:range nbib:Publication ;
         nrl:inverseProperty nbib:isPublicationOf .

   nbib:isPublicationOf
         a       rdf:Property ;
         rdfs:domain nbib:Publication ;
         rdfs:comment "Links a publication to its document." ;
         rdfs:label "isPublicationOf" ;
         rdfs:range nfo:Document ;
         nrl:inverseProperty nbib:publishedAs .

    nbib:citeKey
          a       rdf:Property ;
          rdfs:domain nbib:Reference ;
          rdfs:comment "Used to identify the reference." ;
          rdfs:label "CiteKey" ;
          rdfs:subPropertyOf nie:identifier ;
          rdfs:range xsd:string ;
          nrl:cardinality 1 ;
          nrl:maxCardinality 1 .

    nbib:chapterNumber
          a       rdf:Property ;
          rdfs:domain nbib:Chapter ;
          rdfs:comment "The number of the Chapter for example 1.2, II etc" ;
          rdfs:label "chapterNumber" ;
          rdfs:range xsd:string ;
          nrl:maxCardinality 1 .

    nie:abstract
          a       rdf:Property ;
          rdfs:domain nie:InformationElement ;
          rdfs:comment "An abstract of the work." ;
          rdfs:label "Abstract" ;
          rdfs:range xsd:string ;
          rdfs:subPropertyOf nao:description ;
          nrl:maxCardinality 1 .

    nfo:doi
          a       rdf:Property ;
          rdfs:domain nie:InformationElement ;
          rdfs:comment "The Digital object identifier. see http://www.doi.org/ for more information" ;
          rdfs:label "DOI" ;
          rdfs:range xsd:string ;
          rdfs:subPropertyOf nao:identifier ;
          nrl:maxCardinality 1 .

    nbib:edition
          a       rdf:Property ;
          rdfs:domain nbib:Book, nbib:Manual ;
          rdfs:comment "The edition of a book, long form (such as 'first' or 'second')." ;
          rdfs:label "Edition" ;
          rdfs:range xsd:string ;
          nrl:maxCardinality 1 .

    nco:editor
          a       rdf:Property ;
          rdfs:domain nie:InformationElement ;
          rdfs:comment "The name(s) of the editor(s)." ;
          rdfs:label "Editor" ;
          rdfs:range nco:Contact .

    nbib:eprint
          a       rdf:Property ;
          rdfs:domain nbib:Publication ;
          rdfs:comment "A specification of an electronic publication, often a preprint or a technical report." ;
          rdfs:label "Eprint" ;
          rdfs:range xsd:string ;
          rdfs:subPropertyOf nao:identifier ;
          nrl:maxCardinality 1 .

    nbib:hasArticle
          a       rdf:Property ;
          rdfs:domain nbib:JournalIssue ;
          rdfs:comment "Connects an article to its journal issue" ;
          rdfs:label "hasArticle" ;
          rdfs:subPropertyOf nie:hasLogicalPart ;
          nrl:inverseProperty nbib:journalIssue ;
          rdfs:range nbib:Article .

    nbib:referencedChapter
          a       rdf:Property ;
          rdfs:domain nbib:Reference ;
          rdfs:comment "The chapter number of the reference." ;
          rdfs:label "referencedChapter" ;
          rdfs:range nbib:Chapter ;
          rdfs:subPropertyOf nie:hasLogicalPart ;
          nrl:maxCardinality 1 .

    nbib:chapter
          a       rdf:Property ;
          rdfs:domain nbib:Publication ;
          rdfs:comment "Connects a publication to one of its chapters" ;
          rdfs:label "chapter" ;
          rdfs:subPropertyOf nie:isLogicalPartOf ;
          nrl:inverseProperty nbib:chapterOf ;
          rdfs:range nbib:Chapter .

    nbib:chapterOf
          a       rdf:Property ;
          rdfs:domain nbib:Chapter ;
          rdfs:comment "Connects a chapter to the publication it is from" ;
          rdfs:label "chapterOf" ;
          rdfs:subPropertyOf nie:hasLogicalPart ;
          nrl:inverseProperty nbib:chapterOf ;
          rdfs:range nbib:Publication ;
          nrl:maxCardinality 1 .

    nbib:publication
          a       rdf:Property ;
          rdfs:domain nbib:Reference ;
          rdfs:comment "The publication used for this reference" ;
          rdfs:label "publication" ;
          rdfs:range nbib:Publication ;
          nrl:maxCardinality 1 .

    nbib:publicationMethod
          a       rdf:Property ;
          rdfs:domain nbib:Publication ;
          rdfs:comment "How it was published, if the publishing method is nonstandard." ;
          rdfs:label "publicationMethod" ;
          rdfs:range xsd:string ;
          nrl:maxCardinality 1 .

    nbib:isbn
          a       rdf:Property ;
          rdfs:domain nbib:Publication ;
          rdfs:comment "The International Standard Book Number." ;
          rdfs:label "ISBN" ;
          rdfs:range xsd:string ;
          rdfs:subPropertyOf nao:identifier ;
          nrl:maxCardinality 1 .

    nbib:issn
          a       rdf:Property ;
          rdfs:domain nbib:Series ;
          rdfs:comment "The International Standard Serial Number. Used to identify a series of publications." ;
          rdfs:label "ISSN" ;
          rdfs:range xsd:string ;
          rdfs:subPropertyOf nao:identifier ;
          nrl:maxCardinality 1 .

    nbib:journalIssue
          a       rdf:Property ;
          rdfs:domain nbib:Article ;
          rdfs:comment "The journal issue or magazine the work was published in." ;
          rdfs:label "inJournalIssue" ;
          rdfs:range nbib:JournalIssue ;
          nrl:inverseProperty nbib:hasArticle ;
          nrl:cardinality 1 ;
          nrl:maxCardinality 1 .

    nbib:journal
          a       rdf:Property ;
          rdfs:domain nbib:JournalIssue ;
          rdfs:comment "The journal this issue was published in." ;
          rdfs:label "journal" ;
          rdfs:range nbib:Journal ;
          nrl:inverseProperty nbib:hasIssue ;
          nrl:cardinality 1 ;
          nrl:maxCardinality 1 .

    nbib:hasIssue
          a       rdf:Property ;
          rdfs:domain nbib:Journal ;
          rdfs:comment "Connects one or more issues from the journal they are published from" ;
          rdfs:label "hasIssue" ;
          nrl:inverseProperty nbib:journal ;
          rdfs:range nbib:JournalIssue .

    nbib:lccn
          a       rdf:Property ;
          rdfs:domain nbib:Publication ;
          rdfs:comment "The Library of Congress Call Number." ;
          rdfs:label "LCCN" ;
          rdfs:range xsd:string ;
          rdfs:subPropertyOf nao:identifier ;
          nrl:maxCardinality 1 .

    nbib:mrNumber
          a       rdf:Property ;
          rdfs:domain nbib:Publication ;
          rdfs:comment "The Mathematical Reviews number." ;
          rdfs:label "MRNumber" ;
          rdfs:range xsd:string ;
          rdfs:subPropertyOf nao:identifier ;
          nrl:maxCardinality 1 .

    nbib:issueNumber
          a       rdf:Property ;
          rdfs:domain nbib:Publication ;
          rdfs:comment "The '(issue) number' of a journal, magazine, or tech-report, if applicable. (Most publications have a 'volume', but no 'number' field.)." ;
          rdfs:label "Issue Number" ;
          rdfs:range xsd:string ;
          nrl:maxCardinality 1 .

    nbib:organization
          a       rdf:Property ;
          rdfs:domain nbib:Proceedings ;
          rdfs:comment "The organization that held the conference whre the proceedings comes from" ;
          rdfs:label "Organization" ;
          rdfs:range nco:OrganizationContact ;
          nrl:maxCardinality 1 .

    nbib:pages
          a       rdf:Property ;
          rdfs:domain nbib:Reference ;
          rdfs:comment "Page numbers, separated either by commas or double-hyphens." ;
          rdfs:label "Pages" ;
          rdfs:range xsd:string ;
          nrl:maxCardinality 1 .

    nbib:proceedings
          a       rdf:Property ;
          rdfs:domain nbib:Article ;
          rdfs:comment "The Proceedings that contain the article publication" ;
          rdfs:label "proceedings" ;
          rdfs:range nbib:Proceedings ;
          rdfs:subPropertyOf nie:hasLogicalPart ;
          nrl:inverseProperty nbib:proceedingsOf ;
          nrl:maxCardinality 1 .

    nbib:proceedingsOf
          a       rdf:Property ;
          rdfs:domain nbib:Proceedings ;
          rdfs:comment "Collects all article publications of this Proceedings" ;
          rdfs:label "proceedingsOf" ;
          rdfs:subPropertyOf nie:isLogicalPart ;
          nrl:inverseProperty nbib:proceedings ;
          rdfs:range nbib:Article .

    nie:publicationDate
          a       rdf:Property ;
          rdfs:domain nie:InformationElement ;
          rdfs:comment "The date of the publication" ;
          rdfs:label "Publication Date" ;
          rdfs:range xsd:dateTime ;
          nrl:maxCardinality 1 .

    nbib:inSeries
          a       rdf:Property ;
          rdfs:domain nbib:Book ;
          rdfs:comment "The series of books the book was published in." ;
          rdfs:label "inSeries" ;
          rdfs:range nbib:BookSeries ;
          nrl:inverseProperty nbib:seriesOf ;
          rdfs:subPropertyOf nie:hasLogicalPart ;
          nrl:maxCardinality 1 .

    nbib:seriesOf
          a       rdf:Property ;
          rdfs:domain nbib:BookSeries ;
          rdfs:comment "The book this series belongs to" ;
          rdfs:label "seriesOf" ;
          rdfs:range nbib:Book ;
          rdfs:subPropertyOf nie:hasLogicalPart ;
          nrl:inverseProperty nbib:inSeries .

    nbib:type
          a       rdf:Property ;
          rdfs:domain nbib:Techreport ;
          rdfs:comment "The type of tech-report, for example, 'Research Note'." ;
          rdfs:label "Type" ;
          rdfs:range xsd:string ;
          nrl:maxCardinality 1 .

    nbib:volume
          a       rdf:Property ;
          rdfs:domain nbib:Publication  ;
          rdfs:comment "The volume of a journal or multi-volume book." ;
          rdfs:label "Volume" ;
          rdfs:range xsd:integer ;
          nrl:maxCardinality 1 .
}

<http://www.example.com/nbib_metadata#>
{<http://www.example.com/nbib_metadata#>
          a       nrl:GraphMetadata ;
          nrl:coreGraphMetadataFor
                  nbib: .

nbib:  a       nrl:Ontology ;
          nao:creator <Joerg.Ehrichs@gmx.de> ;
          nao:hasDefaultNamespace
                  "http://www.example.com/nbib#" ;
          nao:hasDefaultNamespaceAbbreviation
                  "nbib" ;
          nao:lastModified "2011-09-16T10:36:58Z" ;
          nao:status "Unstable" ;
          nao:updatable "0 " ;
          nao:version "0.1" ;
          nao:prefLabel "Nepomuk BibTex Ontology" ;
          nao:description "The Nepomuk BibTex Ontology provides vocabulary to express bibliografic information" .

}

























